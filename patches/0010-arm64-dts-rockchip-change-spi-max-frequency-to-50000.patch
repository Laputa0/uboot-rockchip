From 2dd3343e87728de5d56c9682f3e768f5545fe299 Mon Sep 17 00:00:00 2001
From: John Clark <inindev@gmail.com>
Date: Sun, 19 May 2024 14:46:44 +0000
Subject: [PATCH 10/12] arm64: dts: rockchip: change spi-max-frequency to
 50000000 as per vendor device tree

Signed-off-by: John Clark <inindev@gmail.com>
---
 arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi b/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
index 968385622f..7cf87c747b 100644
--- a/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
+++ b/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
@@ -23,7 +23,7 @@
 		reg = <0>;
 		bootph-pre-ram;
 		bootph-some-ram;
-		spi-max-frequency = <24000000>;
+		spi-max-frequency = <50000000>;
 		spi-rx-bus-width = <4>;
 		spi-tx-bus-width = <1>;
 	};
